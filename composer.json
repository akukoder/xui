{
    "name": "akukoder/xui",
    "description": "Laravel Blade component for frontend developer",
    "license": "MIT",
    "authors": [
        {
            "name": "Syahril Zulkefli",
            "email": "syahzul@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/view": "^7.0|^8.0"
    },
    "require-dev": {
        "psy/psysh": "@stable",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Xui\\": "src/"
        }
    },
    "autoload-dev": {
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xui\\XuiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
